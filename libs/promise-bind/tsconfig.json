{
	"compilerOptions": {
		"module": "esnext",
		"target": "es6",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"downlevelIteration": true,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules"]
}
