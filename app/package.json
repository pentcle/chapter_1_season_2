{
	"name": "carbon",
	"version": "1.0.0",
	"description": "",
	"type": "commonjs",
	"main": "src/index.tsx",
	"scripts": {
		"install:solidity": "yarn add global solc",
		"hardhat": "TS_NODE_TRANSPILE_ONLY=1 hardhat",
		"ts-node": "node --experimental-json-modules --es-module-specifier-resolution=node --loader ts-node/esm",
		"compile": "yarn hardhat compile",
		"compile:watch": "nodemon --watch \"contracts/**\" --ext \"sol\" --exec \"yarn compile\"",
		"dev": "yarn webpack serve",
		"start:dev": "yarn hardhat node",
		"deploy": "TS_NODE_TRANSPILE_ONLY=1 yarn hardhat run --network localhost scripts/deploy.ts",
		"deploy:diamond": "TS_NODE_TRANSPILE_ONLY=1 yarn hardhat run --network localhost scripts/deploy-diamond.ts",
		"deploy:watch": "nodemon --watch \"contracts/**\" --ext \"sol\" --exec \"yarn deploy\"",
		"sign": "yarn hardhat run --network localhost scripts/sign.ts",
		"claim": "yarn hardhat run --network localhost scripts/claim.ts",
		"test": "yarn hardhat test",
		"test:watch": "nodemon --watch \"tests/**\" --ext \"ts\" --exec \"yarn test\"",
		"start": "react-scripts start"
	},
	"private": true,
